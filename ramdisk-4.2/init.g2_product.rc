on boot
# LGE_CHANGE_S, [LGE_DATA][WCDMA_Window_Size], hyoseab.song@lge.com, 2013-02-25
# Re-define TCP buffer size for SKT WCDMA network. Overwrite init.rc property
# ReadMin, ReadInitial, ReadMax, WriteMin, WriteInitial, WriteMax,
	setprop net.tcp.buffersize.umts		4094,87380,1220608,4096,16384,1220608
    setprop net.tcp.buffersize.hspa		4094,87380,1220608,4096,16384,1220608
    setprop net.tcp.buffersize.hsupa	4094,87380,1220608,4096,16384,1220608
    setprop net.tcp.buffersize.hsdpa	4094,87380,1220608,4096,16384,1220608
    setprop net.tcp.buffersize.lte     524288,1048576,2560000,524288,1048576,2560000
# LGE_CHANGE_E, [LGE_DATA][WCDMA_Window_Size], hyoseab.song@lge.com, 2013-02-25

# 2012-10-10 kenneth.ryu@lge.com LGP_DATA_RIL_DISABLE_PATIALRETRY [START]
	setprop persist.qcril.disable_retry	true
# 2012-10-10 kenneth.ryu@lge.com LGP_DATA_RIL_DISABLE_PATIALRETRY [END]

