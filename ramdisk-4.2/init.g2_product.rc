# LGE_CHANGE_S LGT_REQ  [smile.kim@lge.com][2012-02-15][BEGIN]
    mkdir /data/data/com.ubivelox.wipiplayer/W 0777 system system
    mkdir /data/LGTContents 0771 system lgt_gid
    mkdir /data/LGTContents/app 0771 system lgt_gid
    mkdir /data/LGTContents/video 0771 system lgt_gid
    mkdir /data/LGTContents/music 0771 system lgt_gid
    mkdir /data/LGTContents/bell 0771 system lgt_gid
    mkdir /data/LGTContents/DLtmp 0771 system lgt_gid
    chmod 771 /system/persist
    chmod 665 /system/persist/LGUPlus_Api.xml
    chmod 775 /system/lib/modules/mwlan_aarp.ko

# [smile.kim@lge.com][2012-02-15] add LGTConents
    chown system lgt_gid /data/data/com.ubivelox.wipiplayer/W
    chown system lgt_gid /data/LGTContents
    chown system lgt_gid /data/LGTContents/video
    chown system lgt_gid /data/LGTContents/music
    chown system lgt_gid /data/LGTContents/bell
    chown system lgt_gid /data/LGTContents/app
    chown system lgt_gid /data/LGTContents/DLtmp
# LGE_CHANGE_E LGT_REQ  [smile.kim@lge.com][2012-02-15][END]

# LGE_CHANGE_S, [yeonjun.kim@lge.com][2013-06-14] set default brightness
    write /sys/class/leds/lcd-backlight/brightness 173

# Bluetooth tethering
    setprop bluetooth.pan true

on init
    # LGE_CHANGE_S, G2-FS@lge.com, 2013-03-23, export external sdcard
    mkdir /storage/external_SD 0000 system system
    export EXTERNAL_ADD_STORAGE /storage/external_SD
    # LGE_CHANGE_E, G2-FS@lge.com, 2013-03-23, export external sdcard

#2013.04.01 LGE_CHANGE_S hyeongjin.kim@lge.com, SMPL count
service smpl_count /system/bin/smpl_count
    class main
	user root
	oneshot
#2013.04.01 LGE_CHANGE_E hyeongjin.kim@lge.com, SMPL count

on post-fs
    chmod 0644 /system/lib/modules/mwlan_aarp.ko
    insmod /system/lib/modules/mwlan_aarp.ko
    chmod 0644 /system/lib/modules/ipsecdrvtl.ko

